{
  "name": "@witnet/price-feeds",
  "version": "3.0.0",
  "description": "Public data feeds sustained by the Witnet Foundation",
  "author": "Witnet Foundation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/witnet/witnet-price-feeds"
  },
  "private": false,
  "type": "commonjs",
  "bin": {},
  "exports": {
    ".": {
      "require": "./dist/src/lib/index.js",
      "default": "./dist/srcLib/index.js",
      "types": "./dist/src/lib/*.d.ts"
    },
    "./assets": "./dist/witnet/assets/index.js"
  },
  "files": [
    "dist",
    "src/bin",
    "witnet",
    ".env_Witnet"
  ],
  "scripts": {
    "avail": "npx witnet radon assets",
    "build": "tsc -p tsconfig.json",
    "fmt": "eslint ./scripts/**/*.js",
    "fmt!": "eslint ./scripts/**/*.js --fix",
    "pfs:check": "node ./scripts/hardhat-task.js pfs:check",
    "pfs:deploy": "node ./scripts/deploy.js",
    "pfs:sla": "node ./scripts/hardhat-task.js pfs:sla",
    "pfs:status": "node ./scripts/hardhat-task.js pfs:status",
    "poller": "node ./src/bin/poller",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "dependencies": {
    "@witnet/ethers": "^1.1.1",
    "@witnet/sdk": "^1.1.1",
    "cross-env": "^10.0.0",
    "dotenv": "^16.6.1",
    "ethrpc-gateway": "^2.2.9",
    "lodash.merge": "^4.6.2",
    "witnet-solidity-bridge": "^2.2.1"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.18.6",
    "chai": "^6.0.1",
    "eslint": "^9.36.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
