{
  "name": "@witnet/price-feeds",
  "version": "3.0.0",
  "description": "Public data feeds sustained by the Witnet Foundation",
  "author": "Witnet Foundation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/witnet/witnet-price-feeds"
  },
  "private": false,
  "type": "commonjs",
  "bin": {},
  "exports": {
    ".": {
      "types": "./dist/src/lib/*.d.ts",
      "require": "./dist/src/lib/index.js",
      "default": "./dist/src/lib/index.js"
    },
    "./assets": "./dist/witnet/assets/index.js",
    "./utils": {
      "types": "./dist/src/lib/*.d.ts",
      "require": "./dist/src/lib/utils.js",
      "default": "./dist/src/lib/utils.js"
    },
    "./settings": "./witnet/settings.json"
  },
  "files": [
    "dist",
    "src/bin",
    "witnet",
    ".env_witnet"
  ],
  "scripts": {
    "avail": "npx witnet radon assets",
    "build": "tsc -p tsconfig.json",
    "fmt": "eslint --ext .js,.ts,.cjs . --ignore-path .gitignore --ignore-pattern _*",
    "fmt!": "eslint --ext .js,.ts,.cjs . --fix --ignore-path .gitignore --ignore-pattern _*",
    "pfs:settle": "node ./src/bin/cli/settle.js",
    "pfs:status": "node ./src/bin/cli/status.js",
    "prepare": "npm run build",
    "test": "vitest"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^22.18.6",
    "@witnet/ethers": "../witnet-solidity",
    "@witnet/sdk": "../witnet-toolkit",
    "chai": "^6.0.1",
    "cross-env": "^10.0.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.36.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^7.2.1",
    "ethrpc-gateway": "^2.2.9",
    "lodash.merge": "^4.6.2",
    "moment": "^2.30.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "witnet-solidity-bridge": "../witnet-solidity-bridge"
  }
}
